word_list={"aardvark", "horse", "wolf"]
import random
chosen_word = random.choice(word_list)

guess = input("Guess a letter ").lower()

for alphabet in chosen_word
    if alphabet == guess:
       print("Correct")
    else:
       print("Incorrect")

display = []
for alphabet in range(len(chosen_word)):
    display+="_"
print(display)
